<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="picogen" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/picogen" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/picogen" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="optimized AthlonXP">
				<Option output="picogen" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-march=athlon-xp" />
					<Add option="-O3" />
					<Add option="-s -O3 -fexpensive-optimizations \" />
					<Add option="-combine  \" />
					<Add option="-ffast-math \" />
					<Add option="-ftree-vectorize \" />
					<Add option="-msse \" />
					<Add option="-funroll-loops -fvariable-expansion-in-unroller \" />
					<Add option="-funsafe-loop-optimizations \" />
					<Add option="-ftree-loop-im \" />
					<Add option="-funswitch-loops \" />
					<Add option="-ftree-loop-ivcanon \" />
					<Add option="-ftracer \" />
					<Add option="-fprefetch-loop-arrays \" />
					<Add option="-freorder-blocks-and-partition \" />
					<Add option="-funsafe-math-optimizations -ffinite-math-only \" />
					<Add option="-fdata-sections \" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`sdl-config --cflags`" />
			<Add directory="../../../include" />
		</Compiler>
